{
  "version": 1,
  "projects": {
    "foo-project": {
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "options": {
            "tsConfig": "src/tsconfig.app.json",
            "assets": []
          }
        }
      },
      "storybook": {
        "builder": "@storybook/angular:start-storybook",
        "options": {
          "browserTarget": "foo-project:build",
          "port": 6006
        }
      }
    }
  },
  "defaultProject": "foo-project"
}
